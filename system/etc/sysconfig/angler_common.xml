<?xml version="1.0" encoding="utf-8"?>

<!-- These are configurations that should exist on angler only. -->
<config>
    <!-- BG-policy whitelist Huawei support apps that target O -->
    <allow-in-power-save package="com.huawei.sarcontrolservice" />
    <allow-in-power-save package="com.huawei.entitlement" />
    <allow-in-power-save package="com.huawei.callstatisticsutils" />

    <!-- These are telephony components that need to run in the background -->
    <allow-in-power-save package="com.qualcomm.atfwd" />
    <allow-in-power-save package="com.qualcomm.embms" />
    <allow-in-power-save package="com.qulacomm.qcrilmsgtunnel" />
</config>
